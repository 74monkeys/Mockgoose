    "bunyan": "~1.3.5",
    "mongoose": "^3.8.25"
  },
  "devDependencies": {
    "mongoose-validator": "~1.0.3",
    "bcrypt": "~0.8.2",
    "grunt": "~0.4.5",
    "grunt-contrib-watch": "0.6.1",
    "grunt-contrib-jshint": "0.11.1",
    "grunt-mocha-test": "0.12.7",
    "node-uuid": "~1.4.3",
    "async": "~0.9.0",
    "grunt-cli": "~0.1.13",
    "chai": "~2.2.0",
    "mocha": "~2.2.4"
  },
  "bugs": {
    "url": "https://github.com/mccormicka/Mockgoose/issues"
