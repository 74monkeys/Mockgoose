  "version": "1.10.7",
  "author": {
    "name": "Anthony McCormick",
    "email": "anthony.mccormick AT gmail.com"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "datastore",
    "nosql",
    "sql",
    "db",
    "database",
    "mock",
    "stub",
    "in memory"
